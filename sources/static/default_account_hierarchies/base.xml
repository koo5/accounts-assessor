<?xml version="1.0" ?>
<accountHierarchy>
	<!-- top-level account has to explicitly specify where it attaches -->
	<Accounts parent_role="root" normal_side="debit">
		<Net_Assets role="Net_Assets">
			<Assets>
				<Current_Assets role="Current_Assets">
					<Cash_and_Cash_Equivalents role="Cash_and_Cash_Equivalents">
						<Banks role="Banks"/>
					</Cash_and_Cash_Equivalents>
				</Current_Assets>
				<Non_Current_Assets role="Non_Current_Assets">
				</Non_Current_Assets>
			</Assets>
			<Liabilities normal_side="credit">
				<Non_Current_Liabilities>
					<Non_Current_Loans/>
				</Non_Current_Liabilities>
				<Current_Liabilities>
					<Current_Loans/>
					<Gst>
						<Gst_Receivable/>
						<Gst_GstPayableReceivable/>
					</Gst>
				</Current_Liabilities>
			</Liabilities>
		</Net_Assets>

		<Equity role="Equity" normal_side="credit">
			<Share_Capital/>
			<Retained_Earnings>
				<Historical_Earnings role="Historical_Earnings">
					<Bank_Opening_Balances/>
				</Historical_Earnings>
				<Current_Earnings role="Current_Earnings"/>
			</Retained_Earnings>
			<Capital_Introduced/>
			<Drawings role="Drawings"/>
		</Equity>

		<Comprehensive_Income role="Comprehensive_Income" normal_side="credit">
			
			<Revenue role="Revenue">
				<Income/>
				<Other_Income>
					<Interest_Income/>
					<Interest_Earned/>
					<Exchange_Gain/>
				</Other_Income>
				<Currency_Movement role="Currency_Movement"/>
			</Revenue>

			<Expenses role="Expenses" normal_side="debit">
				<Bank_Charges/>
				<Accounting_Fees/>
				<Forex_Loss/>
				<Interest_Expenses/>
				<Purchases/>
				<Other_Expenses/>
			</Expenses>

			<Gains role="Gains">
				<Other_Gain/>
				<Clearing_Account/>
			</Gains>

			<Losses normal_side="debit">	
			</Losses>

		</Comprehensive_Income>
	</Accounts>
	<Inventory parent_role="root" role="root!Inventory" normal_side="debit">
	</Inventory>
</accountHierarchy>
