div7a:sheets
	a excel:sheet_set ;
	excel:sheets
		(
			div7a:sheet
		).


div7a:example_sheetset
	a excel:template_sheet_set ;
	excel:is_listed_by_hardcoding true ;
	excel:has_sheet_instances
		(
			[
				excel:sheet_instance_has_sheet_type div7a:sheet ;
				excel:sheet_instance_has_sheet_data div7a:example_data
			]
		).


div7a:example_data
	a excel:template_instance_example;
	div7a:income_year_of_loan_creation [rdf:value "2014"^^xsd:decimal];
	div7a:full_term_of_loan_in_years [rdf:value "7"^^xsd:decimal]
	div7a:principal_amount_of_loan [rdf:value "10000"^^xsd:decimal]
	div7a:lodgment_day_of_private_company [rdf:value "2014-07-01"^^xsd:date]
	div7a:income_year_of_computation [rdf:value "2015"^^xsd:decimal]
	div7a:repayments [rdf:value (
		[
			div7a:date [rdf:value "2014-08-31"^^xsd:date];
			div7a:value [rdf:value "20000.00"^^xsd:decimal]
		]
		[
			div7a:date [rdf:value "2015-05-30"^^xsd:date];
			div7a:value [rdf:value "8000.00"^^xsd:decimal]
		]
	);
