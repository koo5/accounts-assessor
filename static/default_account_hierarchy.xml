<?xml version="1.0" ?>
<accountHierarchy>
	<!-- top-level account has to explicitly specify where it attaches -->
	<Accounts parent_role_parent="" parent_role_child="">
		<NetAssets normal_side="debit">
			<Assets>
				<CurrentAssets>
					<CashAndCashEquivalents>
						<Banks/>
					</CashAndCashEquivalents>				
					<AssetsLivestock>
						<AssetsLivestockAtCost/>
						<AssetsLivestockAtAverageCost/>
					</AssetsLivestock>
				</CurrentAssets>
				<NoncurrentAssets>
					<FinancialInvestments/>
				</NoncurrentAssets>
			</Assets>
			<Liabilities normal_side="credit">
				<NoncurrentLiabilities>
					<NoncurrentLoans/>
				</NoncurrentLiabilities>
				<CurrentLiabilities>
					<CurrentLoans/>
					<Gst>
						<GstReceivable/>
						<GstPayable/>
					</Gst>
				</CurrentLiabilities>
			</Liabilities>
		</NetAssets>

		<Equity normal_side="credit">
			<ShareCapital/>
			<RetainedEarnings>
				<HistoricalEarnings>
					<BankOpeningBalances/>
				</HistoricalEarnings>
				<CurrentEarnings/>
			</RetainedEarnings>
			<CapitalIntroduced/>
			<Drawings/>
		</Equity>
		<ComprehensiveIncome normal_side="credit">
			
			<Revenue>
				<Income/>
				<OtherIncome>
					<InterestIncome/>
					<InterestEarned/>
					<EarningsFinancialInvestments/>
					<ExchangeGain/>
				</OtherIncome>
				<SalesOfLivestock/>
				<RationsRevenue/>
				<CurrencyMovement/>
			</Revenue>

			<Expenses normal_side="debit">
				<BankCharges/>
				<AccountingFees/>
				<ForexLoss/>
				<InterestExpenses/>
				<LivestockAdjustment/>
				<Purchases/>
				<CostOfGoodsLivestock/>
				<OtherExpenses/>
			</Expenses>

			<Gains>
				<InvestmentIncome/>
				<OtherGain/>
				<ClearingAccount/>
			</Gains>
			<Losses normal_side="debit">

			</Losses>

		</ComprehensiveIncome>
	</Accounts>
	<Inventory parent_role_parent="" parent_role_child="" role_parent="" role_child="Inventory" normal_side="debit">
		<LivestockCount role_parent="Inventory" role_child="LivestockCount"/>
	</Inventory>
</accountHierarchy>
