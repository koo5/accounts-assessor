<?xml version="1.0" ?>
<accountHierarchy>
	<!-- top-level account has to explicitly specify where it attaches -->
	<Accounts parent_role="root/root">
		<NetAssets normal_side="debit">
			<Assets>
				<CurrentAssets>
					<CashAndCashEquivalents>
						<Banks role="Accounts/Banks"/>
					</CashAndCashEquivalents>				
				</CurrentAssets>
				<NoncurrentAssets>
				</NoncurrentAssets>
			</Assets>
			<Liabilities normal_side="credit">
				<NoncurrentLiabilities>
					<NoncurrentLoans/>
				</NoncurrentLiabilities>
				<CurrentLiabilities>
					<CurrentLoans/>
					<Gst>
						<GstReceivable/>
						<GstPayable/>
					</Gst>
				</CurrentLiabilities>
			</Liabilities>
		</NetAssets>

		<Equity normal_side="credit">
			<ShareCapital/>
			<RetainedEarnings>
				<HistoricalEarnings>
					<BankOpeningBalances/>
				</HistoricalEarnings>
				<CurrentEarnings/>
			</RetainedEarnings>
			<CapitalIntroduced/>
			<Drawings/>
		</Equity>

		<ComprehensiveIncome normal_side="credit">
			
			<Revenue>
				<Income/>
				<OtherIncome>
					<InterestIncome/>
					<InterestEarned/>
					<EarningsFinancialInvestments/>
					<ExchangeGain/>
				</OtherIncome>
				<CurrencyMovement/>
			</Revenue>

			<Expenses normal_side="debit">
				<BankCharges/>
				<AccountingFees/>
				<ForexLoss/>
				<InterestExpenses/>
				<Purchases/>
				<OtherExpenses/>
			</Expenses>

			<Gains>
				<OtherGain/>
				<ClearingAccount/>
			</Gains>

			<Losses normal_side="debit">	
			</Losses>

		</ComprehensiveIncome>
	</Accounts>
	<Inventory parent_role="root/root" role="root/Inventory" normal_side="debit">
	</Inventory>
</accountHierarchy>
